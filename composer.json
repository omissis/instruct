{
    "name": "friendsofdrupal/instruct",
    "type": "library",
    "description": "A PHP Event-Condition-Action system implementation.",
    "keywords": ["rules", "eca", "event-condition-action", "event"],
    "license": "MIT",
    "authors": [
        {
            "name": "omissis",
            "email": "claudi0.beatric3@gmail.com"
        }
    ],
    "require": {
        "symfony/validator": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "autoload": {
        "psr-4": { "FOD\\Instruct\\": "src/" }
